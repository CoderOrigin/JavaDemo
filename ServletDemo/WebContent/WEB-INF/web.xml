<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
    <display-name>ServletDemo</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>a</servlet-name>
        <servlet-class>lifePeriodDemo.AServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a</servlet-name>
        <url-pattern>/AServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>b</servlet-name>
        <servlet-class>overrideDoGetAndDoPostDemo.BServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>b</servlet-name>
        <url-pattern>/BServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>c</servlet-name>
        <servlet-class>servletConfigDemo.CServlet</servlet-class>
        <init-param>
            <param-name>n1</param-name>
            <param-value>v1</param-value>
        </init-param>
        <init-param>
            <param-name>n2</param-name>
            <param-value>v2</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>c</servlet-name>
        <url-pattern>/CServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>father</servlet-name>
        <servlet-class>initChildServletDemo.fatherServlet</servlet-class>
        <init-param>
            <param-name>n1</param-name>
            <param-value>v1</param-value>
        </init-param>
        <init-param>
            <param-name>n2</param-name>
            <param-value>v2</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>father</servlet-name>
        <url-pattern>/fatherServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>child</servlet-name>
        <servlet-class>initChildServletDemo.childServlet</servlet-class>
        <init-param>
            <param-name>n1</param-name>
            <param-value>v1</param-value>
        </init-param>
        <init-param>
            <param-name>n2</param-name>
            <param-value>v2</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>child</servlet-name>
        <url-pattern>/childServlet</url-pattern>
    </servlet-mapping>
</web-app>